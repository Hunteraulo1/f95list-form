{
  "name": "flist",
  "private": true,
  "version": "3.3.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "rollup:npm": "rollup -c rollup.config.npm.js",
    "build": "vite build && bun run rollup:npm && node postBuildProcess.js",
    "build:push": "bun run build && clasp push",
    "build:deploy": "bun run build:push && clasp deploy -i AKfycbyDkZ_xQfxT8JQNAGqMYqRBeKB0RXfpNufNzhMwPIzthmPszVPtZkWU7E8vRGS5lW7cEg",
    "preview": "vite preview",
    "lint": "bunx @biomejs/biome lint --write .",
    "format": "bunx @biomejs/biome format --write .",
    "check": "bunx @biomejs/biome check --write . && svelte-check --tsconfig ./tsconfig.json",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "lint-staged": {
    "*.{js,ts,svelte,html,css}": "bunx @biomejs/biome lint --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@rollup/plugin-commonjs": "28.0.1",
    "@rollup/plugin-node-resolve": "15.3.0",
    "@sveltejs/vite-plugin-svelte": "4.0.1",
    "@types/cheerio": "0.22.35",
    "@types/google-apps-script": "1.0.85",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.9.0",
    "autoprefixer": "10.4.20",
    "daisyui": "4.12.14",
    "lint-staged": "15.2.10",
    "postcss": "8.4.49",
    "rollup": "4.27.2",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-delete": "2.1.0",
    "svelte": "5.2.2",
    "svelte-check": "4.0.9",
    "svelte-hero-icons": "5.2.0",
    "svelte-routing": "2.13.0",
    "tailwindcss": "3.4.15",
    "typescript": "5.6.3",
    "vite": "5.4.11",
    "vite-plugin-singlefile": "2.0.3",
    "vite-tsconfig-paths": "5.1.2"
  },
  "optionalDependencies": {
    "@lmdb/lmdb-linux-x64": "^3.1.6",
    "@lmdb/lmdb-win32-x64": "^3.1.6",
    "@rollup/rollup-win32-x64-msvc": "4.27.2",
    "@rollup/rollup-linux-x64-gnu": "4.27.2"
  },
  "dependencies": {
    "jest": "29.7.0",
    "zod": "3.23.8"
  },
  "trustedDependencies": ["@biomejs/biome", "esbuild"],
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    },
    "cz-emoji": {
      "skipQuestions": ["scope", "body", "issues", "breaking"]
    }
  }
}
